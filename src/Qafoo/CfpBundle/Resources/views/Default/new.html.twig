{% extends '::base.html.twig' %}

{% block body %}
  <h2>Create a new CFP</h2>
  <form action="{{ path('qafoo_cfp_new') }}" method="post" {{ form_enctype(form) }}>
    {{ form_widget(form) }}
    <input type="submit" value="Submit RFC" />
    <a href="#" class="help" rel="popover"
		data-placement="right"
		data-content="Please only click if you really want to submit a CFP"
		>Help</a>
  </form>
{% endblock %}
